<div class="mt-5">
	<div class="m-5 p-5">
		<h3 class="p-5 text-2xl font-semibold text-left text-gray-900 dark:text-white dark:bg-gray-800 text-center  fs-3">
			Listado de Platos
			<p class="mt-3 text-sm font-normal text-success dark:text-gray-400">Modifica o Elimina Nuestros Platos.</p>
		</h3>
	<table class="w-full text-sm text-left text-gray-500 dark:text-gray-400 ms-auto me-auto">


		
		<thead class="text-xs text-gray-700 uppercase bg-gray-50 dark:bg-gray-700 dark:text-gray-400">
			<tr>
				<th scope="col" class="py-3 px-6 text-center">
				  Nombre
				</th>
				<th scope="col" class="py-3 px-6 text-center">
					Precio
				</th>
				<th scope="col" class="py-3 px-6 text-center">
				  Descripción
				</th>
				<th scope="col" class="py-3 px-3">
					<span class="sr-only">Editar</span>
				</th>
				<th scope="col" class="py-3 px-3">
				  <span class="sr-only">Eliminar</span>
			  </th>
			</tr>
		</thead>
		<tbody>
		  <% platos.forEach(plato => { %>
			<tr class=" border-b dark:bg-gray-800 dark:border-gray-700">
				<th scope="row" class="">
				  <%= plato.nombrePlato %>
				</th>
				<td class="py-4 p-4 px-6 text-center">
				  $ <%= plato.precio %>
				</td>
				<td class="py-4 px-6 text-center">
				  <%= plato.descripcion %>
				</td>
				<td class="py-3 px-3 text-right">
					<button class="btn btn-warning"><a href="/platos/<%= plato.id%>/edit" class="font-medium text-blue-400 dark:text-blue-500 hover:underline">Editar</a></button>
				</td>
		
			  <form class="py-3  text-right" action="platos/<%=plato.id%>/delete?_method=DELETE" method="post" id="form-delete">
				<td>
					<button type="submit" class="btn btn-danger">Eliminar</button>
				</td>
			  </form>

		  </tr>  
  
		<% }) %>
		</tbody>
	</table>
